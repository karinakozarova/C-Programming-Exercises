#include <stdio.h>

int main(){
  // input
  int x = -1,y = -1,w = -1,h = -1;
  /*do{
    scanf("%d",&x);
  } while(x <= 0);

  do{
    scanf("%d",&y);
  } while(y <= 0);

  do{
    scanf("%d",&w);
  } while(w <= 0);

  do{
    scanf("%d",&h);
  } while(h <= 0);
  */
  x = 2,y = 1,w = 5,h = 4;

  int number_of_tiles = (w*h)/(x*y);
  // show cutting cordinates
  int cutting_cordinates_w_x[number_of_tiles];
  int cutting_cordinates_w_y[number_of_tiles];
  int cutting_point = 0,count = 0;

  for (int i = 0; cutting_point<w; ++i)
  {
      cutting_point += x;
      if (cutting_point > w)
      {
        break;
      }
      cutting_cordinates_w_x[i] = cutting_point;
      count++;
  }
  cutting_point = 0;
  int counter = 0;
  for (int i = 0; cutting_point<h; ++i)
  {
      cutting_point += y;
      if (cutting_point > h)
      {
        break;
      }
      cutting_cordinates_w_y[i] = cutting_point;
      counter++;
  }

  // first way to cut them
  printf("\nX cutting points: \n");
  for (int i = 0; i < count; ++i)
  {
      printf("%d\n",cutting_cordinates_w_x[i]);
  }

  printf("\nY cutting points: \n");
  for (int i = 0; i < counter; ++i)
  {
      printf("%d\n",cutting_cordinates_w_y[i]);
  }

  int number_of_tiles_w = count*counter;


  int cutting_cordinates_h_x[number_of_tiles];
  int cutting_cordinates_h_y[number_of_tiles];

  cutting_point = 0,count = 0;

  for (int i = 0; cutting_point <= w; ++i)
  {
      cutting_point += y;
      if (cutting_point > w) break;

      cutting_cordinates_h_y[i] = cutting_point;
      count++;
  }

  printf("\nY cutting points: \n");
  for (int i = 0; i < counter; ++i)
  {
      printf("%d\n",cutting_cordinates_h_y[i]);
  }
  return 0;
}




